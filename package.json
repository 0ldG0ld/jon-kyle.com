{
  "name": "starter-kit",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "serve": "cd public && python -m SimpleHTTPServer 8080",
    "build": "npm run build-css && npm run build-js && enoki build -o public/",
    "build-css": "node site/design | postcss --use autoprefixer --use postcss-clean > public/bundle.css",
    "watch-css": "watch 'node site/design > site/assets/bundle.css' site/design/",
    "build-js": "browserify site/index.js -t require-globify -t enoki/transform -t envify -g yo-yoify -g es2040 -g uglifyify > public/bundle.js",
    "start": "concurrently --kill-others \"enoki dev\" \"npm run watch-css\"",
    "publish": "dat sync public --no-ignoreHidden"
  },
  "author": "Jon-Kyle Mohr",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "brfs": "^1.4.3",
    "browserify": "^14.3.0",
    "budo": "^9.4.7",
    "concurrently": "^3.4.0",
    "envify": "^4.0.0",
    "es2040": "^1.2.5",
    "mkdirp": "^0.5.1",
    "postcss-clean": "^1.0.2",
    "postcss-cli": "^3.1.1",
    "uglifyify": "^3.0.4",
    "watch": "^1.0.2",
    "yo-yoify": "^3.7.0"
  },
  "dependencies": {
    "bel": "^4.6.0",
    "choo": "^6.0.0",
    "create-html": "^3.1.0",
    "enoki": "^1.0.1",
    "escape-html": "^1.0.3",
    "fontfaceobserver": "^2.0.9",
    "gr8": "^2.1.5",
    "join-path": "^1.1.1",
    "lilcss": "0.0.0",
    "markdown-it": "^8.4.0",
    "markdown-it-implicit-figures": "^0.6.0",
    "marked": "^0.3.6",
    "nano-markdown": "^1.2.0",
    "nanocomponent": "^6.2.0",
    "nanomorph": "^4.0.4",
    "nanoraf": "^3.0.1",
    "object-keys": "^1.0.11",
    "object-values": "^1.0.0",
    "recsst": "^1.1.2",
    "require-globify": "^1.4.1",
    "simplemde": "^1.11.2",
    "tags-input": "^1.1.1",
    "xhr": "^2.4.0",
    "xtend": "^4.0.1"
  }
}
