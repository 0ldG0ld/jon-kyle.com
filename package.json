{
  "name": "starter-kit",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "node source/static && npm run build-css && npm run build-js",
    "build-css": "node ./source/design.js | postcss --use autoprefixer --use postcss-clean > public/bundle.css",
    "watch-css": "watch 'node ./source/design.js > public/bundle.css' ./source",
    "build-js": "browserify source/index.js -t envify -t brfs -g yo-yoify -g es2040 -g uglifyify > public/bundle.js",
    "watch-js": "budo source/index.js:bundle.js --dir public --pushstate --live -- -t brfs -t [ envify --NODE_ENV development ]",
    "start": "concurrently --kill-others \"npm run watch-js\" \"npm run watch-css\""
  },
  "author": "Jon-Kyle Mohr",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "brfs": "^1.4.3",
    "browserify": "^14.3.0",
    "budo": "^9.4.7",
    "concurrently": "^3.4.0",
    "envify": "^4.0.0",
    "es2040": "^1.2.5",
    "postcss-clean": "^1.0.2",
    "postcss-cli": "^3.1.1",
    "uglifyify": "^3.0.4",
    "watch": "^1.0.2",
    "yo-yoify": "^3.7.0"
  },
  "dependencies": {
    "bel": "^4.6.0",
    "choo": "^6.0.0-2",
    "create-html": "^3.1.0",
    "fontfaceobserver": "^2.0.9",
    "gr8": "^2.1.5",
    "join-path": "^1.1.1",
    "lilcss": "0.0.0",
    "marked": "^0.3.6",
    "nano-markdown": "^1.2.0",
    "nanocomponent": "^4.0.0",
    "nanomorph": "^4.0.4",
    "object-keys": "^1.0.11",
    "object-values": "^1.0.0",
    "recsst": "^1.1.2",
    "xhr": "^2.4.0",
    "xtend": "^4.0.1"
  }
}
